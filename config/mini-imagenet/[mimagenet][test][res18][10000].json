{
  "data":
  {
    "dataset": "mini-imagenet",
    "root_dir": "dataset/mini-imagenet",
    "tgt_domains": "mini-imagenet",
    "src_domains": null,
    "shape": 84,
    "Nv": 4000,
    "Nl": 10000,
    "Nu": null,
    "Wu": null,
    "Nd": 1.0
  },

  "model":
  {
    "num_layers": 1,
    "num_heads": 4,
    "backbone": "resnet-18",
    "dropout": 0.0,
    "classes": 100,
    "negative_slope": 0.0,
    "pk": 20,
    "l_ratio": 0.5,
    "strategy": "kmeans"
  },

  "loss":
  {
    "mix": 0.5,
    "graph": 2.0,
    "criterion": "log_loss"
  },

  "train":
  {
    "bsl": 64,
    "bsu": 128,
    "lr": 0.04,
    "mom": 0.95,
    "weight_decay": 2e-4,
    "pretrain_iters": 3000,
    "cycle_iters": 75000,
    "end_iters": 30000,
    "coeff_rampup": 75000,
    "update_interval": 250,
    "sample_interval": 500
  },

  "transform":
  {
    "preprocess":
    {
      "type": "mean-std",
      "mean": [0.47745190, 0.44910542, 0.40136664],
      "std": [0.27563721, 0.26619400, 0.28040630]
    },

    "data_augment":
    {
      "N": 2,
      "M": 9,
      "K": 8,
      "T": 0.75
    }
  }
}