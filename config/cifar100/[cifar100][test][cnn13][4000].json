{
  "data":
  {
    "dataset": "cifar100",
    "root_dir": "dataset/cifar-100",
    "tgt_domains": "cifar100",
    "src_domains": null,
    "shape": 32,
    "Nv": 4000,
    "Nl": 4000,
    "Nu": null,
    "Wu": null,
    "Nd": 1.0
  },

  "model":
  {
    "num_layers": 1,
    "num_heads": 4,
    "backbone": "cnn-13",
    "dropout": 0.0,
    "classes": 100,
    "negative_slope": 0.0,
    "pk": 20,
    "l_ratio": 0.5,
    "strategy": "kmeans"
  },

  "loss":
  {
    "mix": 0.5,
    "graph": 2.0,
    "criterion": "log_loss"
  },

  "train":
  {
    "bsl": 64,
    "bsu": 128,
    "lr": 0.04,
    "mom": 0.95,
    "weight_decay": 2e-4,
    "pretrain_iters": 3000,
    "cycle_iters": 75000,
    "end_iters": 30000,
    "coeff_rampup": 75000,
    "update_interval": 250,
    "sample_interval": 500
  },

  "transform":
  {
    "preprocess":
    {
      "type": "zca",
      "config": "config/cifar100_zca.npz"
    },

    "data_augment":
    {
      "N": 2,
      "M": 9,
      "K": 8,
      "T": 0.75
    }
  }
}